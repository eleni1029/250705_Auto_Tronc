  <item ident="WM_ITEM1_1000100000_10013533_1564718064_439393" title="下列何者不為會計恆等式？">
    <presentation label="下列何者不為會計恆等式？">
      <flow>
        <material>
          <mattext>&lt;p&gt;下列何者不為會計恆等式？&lt;/p&gt;</mattext>
        </material>
        <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
          <render_choice shuffle="Yes">
            <response_label ident="1">
              <flow_mat>
                <material>
                  <mattext>資產 = 負債 + 所有者權益</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="2">
              <flow_mat>
                <material>
                  <mattext>資產 = 負債 + 淨值</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="3">
              <flow_mat>
                <material>
                  <mattext>資產 + 收益 = 負債 + 所有者權 – 費損</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="4">
              <flow_mat>
                <material>
                  <mattext>資產 + 費損 = 負債 +所有者權益 + 收益</mattext>
                </material>
              </flow_mat>
            </response_label>
          </render_choice>
        </response_lid>
      </flow>
    </presentation>
