    <item ident="WM_ITEM1_1000100000_10008821_1519970472_236016" title="下列敘述何者正確？">
      <presentation label="下列敘述何者正確？">
        <flow>
          <material>
            <mattext>下列敘述何者正確？</mattext>
          </material>
          <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
            <render_choice shuffle="Yes">
              <response_label ident="1">
