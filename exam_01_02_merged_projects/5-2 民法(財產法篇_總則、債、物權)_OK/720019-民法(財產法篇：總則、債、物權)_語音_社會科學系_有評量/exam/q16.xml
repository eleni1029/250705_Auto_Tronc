    </wm:class>
  </item>
  <item ident="WM_ITEM1_1000100000_10006073_1692581265_350301" title="契約經解除後，契約本身之效力為何？">
    <presentation label="契約經解除後，契約本身之效力為何？">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;契約經解除後，契約本身之效力為何？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_str ident="FIB91" rcardinality="Ordered" rtiming="No">
          <render_fib fibtype="String" prompt="Box" rows="10" columns="50">
            <response_label ident="A"/>
          </render_fib>
        </response_str>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
    </resprocessing>
    <itemfeedback ident="AllCorrect">
      <flow_mat>
        <material>
          <mattext>All correct. Well done.</mattext>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;答：契約一經解除，契約效力溯及消滅。除法律另有規定或契約另有訂定外，契約當事人雙方須依民法第259條負回復原狀之效力。&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
