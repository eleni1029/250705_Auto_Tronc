  <item ident="WM_ITEM1_1000100000_10006073_1692581984_111781" title="何謂委任契約？">
    <presentation label="何謂委任契約？">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;何謂委任契約？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_str ident="FIB91" rcardinality="Ordered" rtiming="No">
          <render_fib fibtype="String" prompt="Box" rows="10" columns="50">
            <response_label ident="A"/>
          </render_fib>
        </response_str>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
    </resprocessing>
    <itemfeedback ident="AllCorrect">
      <flow_mat>
        <material>
          <mattext>All correct. Well done.</mattext>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;答：民法528，稱委任者，謂當事人約定，一方委託他方處理事務，他方允為處理之契約。&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
        <wm:refurl>http://</wm:refurl>
      </solution>
