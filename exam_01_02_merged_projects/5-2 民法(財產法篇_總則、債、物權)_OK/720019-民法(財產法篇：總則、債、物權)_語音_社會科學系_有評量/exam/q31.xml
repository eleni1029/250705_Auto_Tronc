  <item ident="WM_ITEM1_1000100000_10006073_1692582612_622284" title="下列關於地上權的敍述，何者正確？">
    <presentation label="下列關於地上權的敍述，何者正確？">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;下列關於地上權的敍述，何者正確？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
          <render_choice shuffle="Yes">
            <response_label ident="1">
              <flow_mat>
                <material>
                  <mattext>地上權以占有他人土地，且有建築物或其他工作物為必要</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="2">
              <flow_mat>
                <material>
                  <mattext>地上權不以占有他人土地為必要，但必須有建築物或其他工作物為必要</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="3">
              <flow_mat>
                <material>
                  <mattext>地上權以在他人土地有建築物或其他工作物為目的，但不以占有他人土地為必要</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="4">
              <flow_mat>
                <material>
                  <mattext/>
                </material>
              </flow_mat>
            </response_label>
          </render_choice>
        </response_lid>
      </flow>
    </presentation>
