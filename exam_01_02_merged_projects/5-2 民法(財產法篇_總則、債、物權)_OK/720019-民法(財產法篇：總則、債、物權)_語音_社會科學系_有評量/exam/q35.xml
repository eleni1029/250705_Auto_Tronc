    </wm:class>
  </item>
  <item ident="WM_ITEM1_1000100000_10006073_1692583075_488989" title="甲騎機車外出，途中機車故障，乃就地修 ...">
    <presentation label="甲騎機車外出，途中機車故障，乃就地修 ...">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;甲騎機車外出，途中機車故障，乃就地修繕，但是隨身現金不足，無法清償機車被機車店主人留置。其後，該機車為第三人丙所竊。下列關於機車店主人的留置權是否消滅的敍述，何者正確？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
          <render_choice shuffle="Yes">
            <response_label ident="1">
              <flow_mat>
                <material>
                  <mattext>消滅</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="2">
              <flow_mat>
                <material>
                  <mattext>不消滅</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="3">
              <flow_mat>
                <material>
                  <mattext>以可以請求回復機車之占有為前提，留置權繼續存在。</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="4">
              <flow_mat>
                <material>
                  <mattext/>
                </material>
              </flow_mat>
            </response_label>
          </render_choice>
        </response_lid>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
      <respcondition title="Correct">
        <conditionvar>
          <varequal respident="ANS01">3</varequal>
        </conditionvar>
        <setvar action="Set">1</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="Correct"/>
      </respcondition>
    </resprocessing>
    <itemfeedback ident="Correct">
      <flow_mat>
        <material>
          <mattext/>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;説明：留置權人喪失其留置物之占有，未能請求返還者，其留置權消滅。&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
        <wm:refurl>http://</wm:refurl>
      </solution>
      <hint enable="false">
        <hintmaterial>
          <material>
            <mattext/>
          </material>
        </hintmaterial>
      </hint>
    </itemfeedback>
