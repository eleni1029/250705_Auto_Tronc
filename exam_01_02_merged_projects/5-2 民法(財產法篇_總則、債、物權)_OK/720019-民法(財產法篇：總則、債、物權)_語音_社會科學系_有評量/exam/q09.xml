  <item ident="WM_ITEM1_1000100000_10006073_1692580757_355556" title="甲所有已經登記的土地被乙無權占有十七 ...">
    <presentation label="甲所有已經登記的土地被乙無權占有十七 ...">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;甲所有已經登記的土地被乙無權占有十七年。請問：甲得否行使所有物返還請求權？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
          <render_choice shuffle="Yes">
            <response_label ident="1">
              <flow_mat>
                <material>
                  <mattext>可以，且乙不得抗辯</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="2">
              <flow_mat>
                <material>
                  <mattext>不可以</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="3">
              <flow_mat>
                <material>
                  <mattext>可以，但乙可以抗辯</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="4">
              <flow_mat>
                <material>
                  <mattext/>
                </material>
              </flow_mat>
            </response_label>
          </render_choice>
        </response_lid>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
      <respcondition title="Correct">
        <conditionvar>
          <varequal respident="ANS01">1</varequal>
        </conditionvar>
        <setvar action="Set">1</setvar>
        <displayfeedback feedbacktype="Response" linkrefid="Correct"/>
      </respcondition>
    </resprocessing>
    <itemfeedback ident="Correct">
      <flow_mat>
        <material>
          <mattext/>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;説明：已經登記得土地，其返還清求權不適用消滅時效的規定&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
