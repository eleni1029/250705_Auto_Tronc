  <item ident="WM_ITEM1_1000100000_10006073_1692582163_346481" title="合夥契約之經營共同事業是否以營利事業 ...">
    <presentation label="合夥契約之經營共同事業是否以營利事業 ...">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;合夥契約之經營共同事業是否以營利事業為限？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_str ident="FIB91" rcardinality="Ordered" rtiming="No">
          <render_fib fibtype="String" prompt="Box" rows="10" columns="50">
            <response_label ident="A"/>
          </render_fib>
        </response_str>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
    </resprocessing>
    <itemfeedback ident="AllCorrect">
      <flow_mat>
        <material>
          <mattext>All correct. Well done.</mattext>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;答：事業之種類、性質，法無限制，不問其為精神的或經濟的，營利的或公益的，苟不違背公共秩序及善良風俗即可。&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
        <wm:refurl>http://</wm:refurl>
      </solution>
      <hint enable="false">
        <hintmaterial>
          <material>
            <mattext/>
          </material>
        </hintmaterial>
      </hint>
    </itemfeedback>
