  <item ident="WM_ITEM1_1000100000_10006073_1692581211_498041" title="何謂加害給付？">
    <presentation label="何謂加害給付？">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;何謂加害給付？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_str ident="FIB91" rcardinality="Ordered" rtiming="No">
          <render_fib fibtype="String" prompt="Box" rows="10" columns="50">
            <response_label ident="A"/>
          </render_fib>
        </response_str>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
    </resprocessing>
    <itemfeedback ident="AllCorrect">
      <flow_mat>
        <material>
          <mattext>All correct. Well done.</mattext>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;答：民法227 II，指債務人之給付因而致債權人之其他固有權益受有損害者而言，例如給付病犬，致傳染債權人原有之他犬而死亡者是。&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
        <wm:refurl>http://</wm:refurl>
      </solution>
      <hint enable="false">
        <hintmaterial>
          <material>
            <mattext/>
          </material>
        </hintmaterial>
      </hint>
    </itemfeedback>
