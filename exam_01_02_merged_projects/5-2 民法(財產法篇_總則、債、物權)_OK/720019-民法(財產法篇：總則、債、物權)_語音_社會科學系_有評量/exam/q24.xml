  <item ident="WM_ITEM1_1000100000_10006073_1692582132_104082" title="記名之物品運送提單得否以背書方式，移 ...">
    <presentation label="記名之物品運送提單得否以背書方式，移 ...">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;記名之物品運送提單得否以背書方式，移轉於他人？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_str ident="FIB91" rcardinality="Ordered" rtiming="No">
          <render_fib fibtype="String" prompt="Box" rows="10" columns="50">
            <response_label ident="A"/>
          </render_fib>
        </response_str>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
    </resprocessing>
    <itemfeedback ident="AllCorrect">
      <flow_mat>
        <material>
          <mattext>All correct. Well done.</mattext>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;答：除非提單上有禁止背書之記載，否則得轉讓之。民法628，提單縱為記名式，仍得以背書移轉於他人。但提單上有禁止背書之記載者，不在此限。&lt;br /&gt;
&amp;nbsp;&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
        <wm:refurl>http://</wm:refurl>
      </solution>
      <hint enable="false">
        <hintmaterial>
          <material>
            <mattext/>
          </material>
        </hintmaterial>
      </hint>
    </itemfeedback>
