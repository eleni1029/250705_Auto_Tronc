  <item ident="WM_ITEM1_1000100000_10006073_1692582205_411827" title="何謂合會？">
    <presentation label="何謂合會？">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;何謂合會？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_str ident="FIB91" rcardinality="Ordered" rtiming="No">
          <render_fib fibtype="String" prompt="Box" rows="10" columns="50">
            <response_label ident="A"/>
          </render_fib>
        </response_str>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
    </resprocessing>
    <itemfeedback ident="AllCorrect">
      <flow_mat>
        <material>
          <mattext>All correct. Well done.</mattext>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;答：民法709-1 I，依民法規定，合會契約之類型有二。一為由會首邀集二人以上為會員，互約交付會款及標取合會金之契約；另一為由會首個別與會員互約交付會款，並標取合會金之契約。&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
        <wm:refurl>http://</wm:refurl>
      </solution>
      <hint enable="false">
        <hintmaterial>
          <material>
            <mattext/>
          </material>
        </hintmaterial>
      </hint>
    </itemfeedback>
    <wm:hardlevel>3</wm:hardlevel>
