  <item ident="WM_ITEM1_1000100000_10006073_1692580520_940491" title="甲誤以爲某字畫是大書法家文徴明的真跡 ...">
    <presentation label="甲誤以爲某字畫是大書法家文徴明的真跡 ...">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;甲誤以爲某字畫是大書法家文徴明的真跡而購買之，經鑑定確定是仿製的贋品。下列關於法律效果的敍述，何者正確？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
          <render_choice shuffle="Yes">
            <response_label ident="1">
              <flow_mat>
                <material>
                  <mattext>有效，但是以甲沒有過失爲限</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="2">
              <flow_mat>
                <material>
                  <mattext>無效</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="3">
              <flow_mat>
                <material>
                  <mattext>得撤銷</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="4">
              <flow_mat>
                <material>
                  <mattext/>
                </material>
              </flow_mat>
            </response_label>
          </render_choice>
        </response_lid>
      </flow>
