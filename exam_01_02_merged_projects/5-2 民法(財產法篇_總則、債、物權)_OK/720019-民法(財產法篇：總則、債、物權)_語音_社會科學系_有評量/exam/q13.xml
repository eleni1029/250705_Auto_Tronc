  <item ident="WM_ITEM1_1000100000_10006073_1692580925_672298" title="民法侵權行為分為哪兩大類群？">
    <presentation label="民法侵權行為分為哪兩大類群？">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;b&gt;&lt;font color="#000000" size="2"&gt;民法侵權行為分為哪兩大類群？&lt;/font&gt;&lt;/b&gt;&lt;/p&gt;</mattext>
        </material>
        <response_str ident="FIB91" rcardinality="Ordered" rtiming="No">
          <render_fib fibtype="String" prompt="Box" rows="10" columns="50">
            <response_label ident="A"/>
          </render_fib>
        </response_str>
      </flow>
    </presentation>
    <resprocessing>
      <outcomes>
        <decvar vartype="Integer" defaultval="0"/>
      </outcomes>
    </resprocessing>
    <itemfeedback ident="AllCorrect">
      <flow_mat>
        <material>
          <mattext>All correct. Well done.</mattext>
        </material>
      </flow_mat>
      <solution>
        <solutionmaterial>
          <material>
            <mattext>&lt;p&gt;答案：－般侵權行為類型，民法184：特殊侵權行為類型，民法185至191之3&lt;/p&gt;</mattext>
          </material>
        </solutionmaterial>
