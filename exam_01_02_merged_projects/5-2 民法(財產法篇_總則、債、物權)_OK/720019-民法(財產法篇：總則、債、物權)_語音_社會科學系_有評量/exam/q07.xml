        <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
          <render_choice shuffle="Yes">
            <response_label ident="1">
              <flow_mat>
                <material>
                  <mattext>買賣契約無效，古董所有權屬於甲</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="2">
              <flow_mat>
                <material>
                  <mattext>買賣契約無效，但古董所有權屬於乙</mattext>
