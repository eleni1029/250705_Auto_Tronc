  <item ident="WM_ITEM1_1000100000_10006073_1688991464_989943" title="主要是顧客利用電話主動來電要求服務的 ...">
    <presentation label="主要是顧客利用電話主動來電要求服務的 ...">
      <flow>
        <material>
          <mattext>&lt;p&gt;&lt;font color="#000000" size="2"&gt;主要是顧客利用電話主動來電要求服務的機制，是屬於顧客服務中心哪一層面的功能？&lt;/font&gt;&lt;/p&gt;</mattext>
        </material>
        <response_lid ident="ANS01" rcardinality="Single" rtiming="No">
          <render_choice shuffle="Yes">
            <response_label ident="1">
              <flow_mat>
                <material>
                  <mattext>進線服務(inbound service)</mattext>
                </material>
              </flow_mat>
            </response_label>
            <response_label ident="2">
              <flow_mat>
                <material>
                  <mattext>外撥服務(outbound service)</mattext>
                </material>
              </flow_mat>
            </response_label>
